{
  "name": "math-skills-game",
  "version": "1.0.0",
  "description": "Math skills game built with react",
  "main": "index.js",
  "scripts": {
    "build:server": "babel ./src/server.js -d ./build",
    "build:client": "webpack --mode production",
    "heroku-postbuild": "yarn build:client && yarn build:server",
    "start": "node ./build/src/server.js",
    "start:dev": "webpack-dev-server --mode=development --inline --hot",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "repository": "https://github.com/Dheavyman/math-skills-game.git",
  "author": "Justin Nebo <nebojustin@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ]
